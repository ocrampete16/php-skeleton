{
    "name": "marcopetersen/php-skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Petersen",
            "email": "m@rcopetersen.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "localheinz/composer-normalize": "^1.0",
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Skeleton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Skeleton\\Test\\": "tests/"
        }
    }
}
